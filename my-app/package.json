{
  "name": "sakura-launcher",
  "productName": "SakuraLauncher",
  "author": "SakuraProject",
  "version": "2.1.0",
  "description": "Minecraft Launcher",
  "main": "./public/electron.js",
  "repository": "https://github.com/20GK/SakuraLauncherV4",
  "publish": {
    "provider": "github",
    "releaseType": "release"
  },
  "homepage": "./",
  "scripts": {
    "react": "react-scripts start",
    "electron": "electron . --disable-gpu --trace-warnings",
    "build-Dev": "react-scripts build && electron-builder --win",
    "build-Publish": "react-scripts build && electron-builder --win -p always"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@vercel/webpack-asset-relocator-loader": "^1.7.3",
    "babel-loader": "^9.1.2",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.7.3",
    "electron": "22.2.0",
    "electron-builder": "^23.6.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.7.2",
    "node-loader": "^2.0.0",
    "sass": "^1.58.0",
    "sass-loader": "^13.2.0",
    "style-loader": "^3.3.1",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1"
  },
  "dependencies": {
    "@electron/remote": "^2.0.9",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "electron-is-dev": "^2.0.0",
    "electron-updater": "^5.3.0",
    "minecraft-launcher-core": "^3.16.18",
    "node-fetch": "^3.3.0",
    "path-browserify": "^1.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.0",
    "react-scripts": "^5.0.1",
    "web-vitals": "^3.1.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "build": {
    "appId": "sakura.launcher.com",
    "productName": "Sakura Launcher",
    "mac": {
      "icon": "build/logo512.png",
      "target": "tar.gz"
    },
    "linux": {
      "category": "Utils",
      "icon": "build/icon.png",
      "packageCategory": "utils",
      "maintainer": "your name <your email>",
      "asar": true,
      "target": [
        "deb",
        "tar.xz"
      ],
      "synopsis": "Basically same as description but shorter"
    },
    "win": {
      "icon": "build/icon.png",
      "target": "nsis"
    },
    "publish": [
      {
        "provider": "github",
        "owner": "20GK",
        "repo": "SakuraLauncherV4",
        "private": false
      }
    ]
  }
}
